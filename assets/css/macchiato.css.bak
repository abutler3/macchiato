/*
*
*  Navigation
*
*/
/* Wrapper / Inherited */

.wrapper-nav-site {
    line-height: 25px;
    color: #4b4b4b;
    margin-bottom: 15px;
    font-family: 'Futura Plus W08', "Century Gothic", CenturyGothic, "Apple Gothic", AppleGothic, "URW Gothic L", "Avant Garde", sans-serif;
    letter-spacing: normal;
    font-weight: 100;
    font-size: 18px;
}
.wrapper-nav-site a {
    font-weight: 100;
}
.wrapper-nav-site a,
.wrapper-nav-site a:visited,
.wrapper-nav-site a:hover,
.wrapper-nav-site a:focus {
    color: #4b4b4b;
}
.wrapper-nav-site a:hover,
.wrapper-nav-site a:focus,
.wrapper-nav-site li.faux-focus a {
    text-decoration: none;
    background: rgb(220, 220, 220);
    /* fallback */

    background: rgba(220, 220, 220, .5);
}
.wrapper-nav-site a.active {
    color: #4b4b4b;
    letter-spacing: -1px;
    font-family: 'Futura Plus W08', "Century Gothic", CenturyGothic, "Apple Gothic", AppleGothic, "URW Gothic L", "Avant Garde", sans-serif;
    font-weight: 300;
    letter-spacing: normal;
}
nav.nav-site {
    position: relative;
    z-index: 11;
    margin: 0;
    *zoom: 1;
    margin-top: -15px;
}
/* clear */

nav.nav-site:before,
nav.nav-site:after {
    display: table;
    content: "";
    line-height: 0;
}
nav.nav-site:after {
    clear: both;
}
/* Top Row with collapse buttons */

nav.nav-site > ul {
    *zoom: 1;
    border: 0;
    padding: 0;
    margin: 0 0 0 -10px;
    text-align: center;
    /* Responsive */
}
/* Centered at small sizes */

@media (min-width: 500px) {
    nav.nav-site > ul {
        text-align: left;
    }
}
nav.nav-site > ul > li {
    width: auto;
    margin-right: 3px;
    display: inline;
    display: inline-block;
    padding: 0;
    list-style-type: none;
    border: 0;
    /* Responsive */

    margin-left: 3px;
    margin-right: 3px;
}
nav.nav-site > ul > li:last-child {
    margin-right: 0;
}
nav.nav-site > ul > li.last-child {
    margin-right: 0;
}
nav.nav-site > ul > li > a {
    -webkit-transition: border 600ms cubic-bezier(0.23, 1, 0.32, 1);
    transition: border 600ms cubic-bezier(0.23, 1, 0.32, 1);
    border: 1px solid transparent;
    padding: 5px 6px 3px 6px;
    margin: 2px 0 2px 0;
    display: inline;
    display: inline-block;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    color: #4b4b4b;
}
/* Touch only */
/*
html.touch nav.nav-site > ul > li > a {
  border-color: #DDD;
}
*/

.front.has-featured .banner .wrapper-nav-site nav.nav-site > ul > li > a:hover,
.front.has-featured .banner .wrapper-nav-site nav.nav-site > ul > li > a:focus,
.wrapper-nav-site nav.nav-site > ul > li > a:hover,
.wrapper-nav-site nav.nav-site > ul > li > a:focus {
    background: none;
}
nav.nav-site > ul > li.dropdown > a {
    -webkit-border-top-right-radius: 0px;
    -moz-border-radius-topright: 0px;
    border-top-right-radius: 0px;
    -webkit-border-bottom-right-radius: 0px;
    -moz-border-radius-bottomright: 0px;
    border-bottom-right-radius: 0px;
    border-right-width: 0;
    padding-right: 3px;
    /* reduce right padding slightly */
}
/* Touch only */
/*
html.touch nav.nav-site > ul > li.dropdown > a  {
  padding-right: 12px;
}
*/

nav.nav-site > ul > li.dropdown > a.dropdown-toggle {
    font-size: 12px;
    vertical-align: top;
    margin-left: -5px;
    display: inline;
    display: inline-block;
    border-right-width: 1px;
    -webkit-border-top-right-radius: 5px;
    -moz-border-radius-topright: 5px;
    border-top-right-radius: 5px;
    -webkit-border-bottom-right-radius: 5px;
    -moz-border-radius-bottomright: 5px;
    border-bottom-right-radius: 5px;
    -webkit-border-top-left-radius: 0px;
    -moz-border-radius-topleft: 0px;
    border-top-left-radius: 0px;
    -webkit-border-bottom-left-radius: 0px;
    -moz-border-radius-bottomleft: 0px;
    border-bottom-left-radius: 0px;
    padding-right: 6px;
    /* undo change set above */
}
/* Touch only */
/*
html.touch nav.nav-site > ul > li.dropdown > a.dropdown-toggle {
  padding-left: 15px;
  padding-right: 15px;
}
*/

.front.has-featured .banner .wrapper-nav-site nav.nav-site > ul > li.dropdown > a.dropdown-toggle:hover,
.front.has-featured .banner .wrapper-nav-site nav.nav-site > ul > li.dropdown > a.dropdown-toggle:focus,
.wrapper-nav-site nav.nav-site > ul > li.dropdown > a.dropdown-toggle:hover,
.wrapper-nav-site nav.nav-site > ul > li.dropdown > a.dropdown-toggle:focus {
    background-color: #eeeeee;
    color: #4b4b4b;
    text-decoration: none;
}
nav.nav-site > ul > li:hover a,
nav.nav-site > ul > li:focus a {
    border-color: #c4c3bf;
}
nav.nav-site > ul > li.open a {
    border-color: #c4c3bf;
}
/* Use close icon when open. */

nav.nav-site .dropdown.open > .dropdown-toggle i:before {
    content: "\f00d";
}
/* Dropdown Menus */

nav.nav-site > ul ul {
    padding: 5px 0;
    /* Add a bit of spacing so rounded corners aren't cut off on hover */
}
nav.nav-site li li {
    padding: 0;
    text-align: left;
}
nav.nav-site li li a {
    font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
    font-weight: normal;
    padding: 7px 20px;
    display: block;
}
nav.nav-site li li a.btn {
    display: inline-block;
}
nav.nav-site li.active li a {
    font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
    font-weight: normal;
}
nav.nav-site li li.active > a {
    font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
    font-weight: bold;
}
.wrapper-nav-site nav.nav-site li li a.active {
    font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
    font-weight: bold;
}
nav.nav-site .dropdown-menu {
    background: rgb(255, 255, 255);
    /* Fallback for < IE 9 */

    background: rgba(255, 255, 255, .94);
    max-width: 250px;
}
nav.nav-site .dropdown-menu ul {
    list-style: none;
}
nav.nav-site.nojs li {
    display: block;
}
/*
*
* Special Menu Items
*
*/

nav li i.fa-lock {
    padding-right: 3px;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
    /* IE 8 */

    filter: alpha(opacity=100);
    /* IE 5-7 */

    -moz-opacity: 1;
    /* Netscape */

    -khtml-opacity: 1;
    /* Safari 1.x */

    opacity: 1;
    font-size: .9em;
    color: #6b6b6b;
}
/* A span with this class is inserted by Barista Special Menu Items <divider_line> */

.divider_line {
    height: 1px;
    margin: 9px 0;
    overflow: hidden;
    background-color: #e5e5e5;
    display: block;
}
.sidenav .divider_line {
    background-color: #ccc;
}
.divider_text {
    padding: 7px 20px 0 20px;
