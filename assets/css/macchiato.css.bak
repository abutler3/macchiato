/*
* This is an embedded font face with the V logo (and only the v logo)
*/

@font-face {
    font-family: 'vanderbilt-v';
    src: url('../fonts/vanderbilt-v/vanderbilt-v.eot');
}
@font-face {
    font-family: 'vanderbilt-v';
    src: url(data:application/x-font-woff;charset=utf-8;base64,) format('woff'), url('../fonts/vanderbilt-v/vanderbilt-v.ttf') format('truetype'), url('../fonts/vanderbilt-v/vanderbilt-v.svg#vanderbilt-v') format('svg');
    font-weight: normal;
    font-style: normal;
}
.icon-v:before {
    font-family: 'vanderbilt-v';
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    /* Better Font Rendering =========== */

    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    content: "\e000";
}
/*
*
* Margins are calculated to try to keep to a 20px vertical grid
* font-size * line-height = total height of the text block, then use margins to make multiple of 20, or 10
*/

h1,
.h1 {
    font-size: 26px;
    margin-top: 23.1px;
    margin-bottom: 23.1px;
}
h2,
.h2 {
    font-size: 23px;
    margin-top: 25px;
    margin-bottom: 25px;
}
h3,
.h3 {
    font-size: 20px;
    margin-top: 17x;
    margin-bottom: 17px;
}
h4,
.h4 {
    font-size: 18px;
    margin-top: 18.3px;
    margin-bottom: 18.3px;
}
h5,
.h5 {
    font-size: 16px;
    margin-top: 19.6px;
    margin-bottom: 19.6px;
}
h6,
.h6 {
    font-size: 14px;
    margin-top: 20.9px;
    margin-bottom: 20.9px;
}
