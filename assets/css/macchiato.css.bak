.faculty-directory-search-instructions i {
    display: block;
    position: absolute;
    -webkit-animation: slowbounce 3s infinite;
    /* Safari 4+ */

    -moz-animation: slowbounce 3s infinite;
    /* Fx 5+ */

    -o-animation: slowbounce 3s infinite;
    /* Opera 12+ */

    animation: slowbounce 3s infinite;
