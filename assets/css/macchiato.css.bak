/*
*
*  Shared across multiple pages
*
*/

.rss-link {
    text-align: center;
    margin: 1.4em 0;
}
.rss-link i {
    padding-left: 5px;
}
.well > h2 {
    margin-top: 0;
}
.two-column-wrap {
    column-count: 2;
    column-gap: 20px;
    -moz-column-count: 2;
    -moz-column-gap: 20px;
    -webkit-column-count: 2;
    -webkit-column-gap: 20px;
}
.clickover-trigger,
.clickover-trigger:hover,
.clickover-trigger:visited {
    border-bottom: 1px dotted #6b6b6b;
    cursor: pointer;
    color: #6b6b6b;
    text-decoration: none;
}
.popover {
    min-width: 300px;
}
.popover-title {
    font-size: 18px;
}
.region-content .read-more-link a {
    background: #fff;
    border-radius: 4px;
    padding: 8px 10px;
    display: inline;
    display: inline-block;
    -webkit-font-smoothing: antialiased;
    /* Light text on dark backgrounds */
}
.region-content .read-more-link a:hover {
    text-decoration: none;
}
.read-more-link,
.read-more-link-inverted {
    text-align: right;
    margin-top: 1.4em;
    /* Added for featured content blocks. */
}
.read-more-link i {
    font-size: 12px;
}
.item-list .pager {
    margin-top: 1em;
}
.item-list ul.pager li {
    padding: 0;
}
/* Resolves an odd bug in IE 9 with giant underlines */

section.collapsible .collapsible-header a,
section.collapsible .collapsible-header a span,
section.collapsible .collapsible-header a:hover span {
    text-decoration: none;
}
/* Fixes an issue where these imported and are fixed width size */

.wp-caption {
    max-width: 100%;
}
/*
*
* Administrative content
*/

.nav-pills a,
.nav-pills a:visited {
    color: #428bca;
}
.nav-pills a:hover,
.nav-pills a:focus {
    color: #2a6496;
    text-decoration: underline;
}
/*
* Fixes an issue where admin-menu sub-sub-menus are behind content.
*/

body #admin-menu .dropdown li li {
    opacity: 1;
    float: none;
}
/*
*
* Print Styles
*
*/

@media print {
    html body {
        margin-top: 0 !important;
        padding-top: 0 !important;
    }
    .barista-adminbar,
    .topbar,
    #siteNavWrapper,
    .drupal-tabs {
        display: none !important;
    }
    .header-site a:after {
        content: "";
    }
    a[href]:after {
        font-size: .8em;
        -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=90)";
        /* IE 8 */

        filter: alpha(opacity=90);
        /* IE 5-7 */

        -moz-opacity: 0.9;
        /* Netscape */

        -khtml-opacity: 0.9;
        /* Safari 1.x */

        opacity: 0.9;
    }
    .wrapper-form-search {
        display: none;
    }
    .sidebar #block-barista-block-placement-sidebar-first .block {
        max-width: 400px;
        margin-right: 40px;
        float: left;
    }
    .sidebar #block-barista-block-placement-sidebar-first .block:nth-child(2) {
        clear: left;
    }
    .footer-logo-container a:after {
        display: block;
    }
    .footer a {
        white-space: nowrap;
    }
}

