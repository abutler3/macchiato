}
.directory-large-icon svg path {
    stroke-dasharray: 3400px;
    stroke-dashoffset: 3400px;
    -webkit-animation-name: drawsvg;
    -webkit-animation-duration: 3s;
    -webkit-animation-fill-mode: forwards; // Stay on the last frame
    -webkit-animation-iteration-count: 1; // Run only once
    -webkit-animation-timing-function: ease-out;
    animation-name: drawsvg;
    animation-duration: 3s;
    animation-fill-mode: forwards; // Stay on the last frame
    animation-iteration-count: 1; // Run only once
    animation-timing-function: ease-out;
}
.directory-large-icon svg g:nth-child(1) path {
    -webkit-animation-delay: 1s;
    -webkit-animation-duration: 5s;
}
.directory-large-icon svg g:nth-child(2) path {
    -webkit-animation-delay: 0s;
    -webkit-animation-duration: 5s;
}
@-o-keyframes drawsvg {
    to {
        stroke-dashoffset: 0;
    }
}
@-moz-keyframes drawsvg {
    to {
        stroke-dashoffset: 0;
    }
}
@-webkit-keyframes drawsvg {
    to {
        stroke-dashoffset: 0;
    }
}
@keyframes drawsvg {
    to {
        stroke-dashoffset: 0;
    }
}
